{
    "name": "adnane-ka/omnipay-tap",
    "description": "Tap payments gateway for Omnipay payment processing library",
    "type": "library",
    "license": "MIT",
    "keywords": [
      "omnipay",
      "tap",
      "payment",
      "gateway",
      "integration"
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Tap\\": "src/",
            "Omnipay\\Tap\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Adnane Kadri",
            "email": "dannykadri0@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "omnipay/common": "^3.3",
        "symfony/http-client": "^6.2",   
        "php-http/httplug": "^2.3",       
        "nyholm/psr7": "^1.8",            
        "php-http/curl-client": "^2.1",   
        "guzzlehttp/psr7": "^2.4",        
        "php-http/message": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}